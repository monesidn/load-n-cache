    "eslint": "^8.22.0",
    "jest": "^28.1.3",
    "jest-localstorage-mock": "^2.4.22",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.7",
    "typedoc": "^0.23.10",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "eventemitter3": "^4.0.7"
